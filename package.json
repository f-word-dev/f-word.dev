{
  "private": true,
  "scripts": {
    "start": "eleventy --serve --quiet",
    "code": "editorconfig-checker",
    "html": "node-w3c-validator -f lint -evH -i dist/**/*.html",
    "build": "rm -rf dist && eleventy && gulp",
