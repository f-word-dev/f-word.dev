{
  "private": true,
  "scripts": {
    "start": "eleventy --serve --quiet",
    "build": "rm -rf dist && eleventy",
    "test": "npm run editorconfig",
    "editorconfig": "editorconfig-checker",
    "html": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --filterfile .vnurc dist/**/*.html",
    "deploy": "cd dist && rsync --progress --archive --compress --delete --exclude=.DS_Store . f-word.dev:/var/www/f-word.dev/html/"
  },
  "engines": {
    "node": "16"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "editorconfig-checker": "^5.0.1",
    "esbuild": "^0.17.15",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it-named-headings": "^1.1.0",
    "music-metadata": "7.12.3",
    "postcss": "^8.4.21",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-media-minmax": "^5.0.0",
    "pretty-data": "^0.40.0",
    "vnu-jar": "^22.9.29"
  }
}
