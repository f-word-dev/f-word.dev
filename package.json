{
  "private": true,
  "scripts": {
    "start": "eleventy --serve --quiet",
    "code": "editorconfig-checker",
    "html": "node-w3c-validator -f lint -evH -i dist/**/*.html",
    "build": "rm -rf dist && eleventy && gulp",
    "deploy": "cd dist && rsync --archive --compress --delete --exclude=.DS_Store . f-word.dev:/var/www/f-word.dev/html/"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "browserslist": [
    "last 2 version",
    "not dead",
    "not ie <= 11",
    "iOS >= 12"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "del": "^6.0.0",
    "editorconfig-checker": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-rev-rewrite": "^5.0.0",
    "gulp-terser": "^2.1.0",
    "html-minifier": "^4.0.0",
    "markdown-it-named-headings": "^1.1.0",
    "music-metadata": "^7.11.4",
    "postcss": "^8.3.9",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^14.0.2",
    "pretty-data": "^0.40.0",
    "rollup-stream": "^1.24.1",
