    "not dead",
    "not ie <= 11",
    "iOS >= 12"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "del": "6.1.1",
    "editorconfig-checker": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "9.0.0",
    "gulp-rev-rewrite": "^5.0.0",
    "html-minifier": "^4.0.0",
    "markdown-it-named-headings": "^1.1.0",
    "music-metadata": "7.12.3",
    "postcss": "^8.3.9",
    "postcss-csso": "^6.0.0",
    "postcss-import": "^15.0.0",
    "pretty-data": "^0.40.0",
    "rollup": "^3.4.0",
    "vinyl-paths": "3.0.1"
